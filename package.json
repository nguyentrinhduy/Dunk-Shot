{
    "name": "template-webpack",
    "version": "1.2.0",
    "main": "src/main.js",
    "scripts": {
        "dev": "webpack-dev-server --config webpack/config.dev.js --open",
        "build": "rimraf dist && webpack --config webpack/config.prod.js",
        "util:format": "prettier --write src/**",
        "deploy": "gh-pages -d dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phaserjs/template-webpack.git"
    },
    "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/phaserjs/template-webpack/issues"
    },
    "homepage": "https://github.com/phaserjs/template-webpack#readme",
    "prettier": {
        "tabWidth": 4,
        "printWidth": 100,
        "trailingComma": "es5",
        "semi": false,
        "singleQuote": true,
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "always",
        "endOfLine": "lf"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@typescript-eslint/parser": "^5.30.6",
        "eslint": "^8.42.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "rimraf": "^3.0.2",
        "prettier": "^2.5.1",
        "prettier-eslint": "^15.0.1",
        "@babel/core": "^7.24.5",
        "@babel/preset-env": "^7.24.5",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "file-loader": "^6.2.0",
        "gh-pages": "^6.1.1",
        "html-webpack-plugin": "^5.6.0",
        "raw-loader": "^4.0.2",
        "terser-webpack-plugin": "^5.3.10",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.5",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "webpack-merge": "^5.10.0"
    },
    "dependencies": {
        "phaser": "^3.80.1"
    }
}
